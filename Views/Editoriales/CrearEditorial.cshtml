<div class="content-wrapper">

    <h1>

        Administrar Editoriales

    </h1>



    <h3>Ingresar Nuevo Editorial</h3>

    <form asp-controller="Editoriales" asp-action="CrearEditorial" method="post">



        <div class="col">
            <label>CÓDIGO DE EDITORIAL </label>
            <input type="text" class="form-control" id="id_editorial" name="id_editorial">

        </div>

        <div class="col">
            <label>NOMBRE DE EDITORIAL</label>
            <input type="text" class="form-control" id="nombreEditorial" name="nombreEditorial">

        </div>

        <br />
        <div class="btn-group">
            <div class="col">
                <button type="button" class="btn btn-block bg-gradient-danger btn-sm">Borrar</button>
            </div> &nbsp;&nbsp;&nbsp;&nbsp; <div class="col"> <button type="submit" class="btn btn-block bg-gradient-success btn-sm">Crear</button></div>
        </div>







    </form>


</div>



